{
  "name": "@verdocs/js-sdk",
  "version": "1.0.3",
  "private": false,
  "homepage": "https://github.com/Verdocs/js-sdk",
  "description": "Verdocs JS SDK",
  "keywords": [
    "pdf",
    "document",
    "signing",
    "Verdocs",
    "embed"
  ],
  "author": "Verdocs, LLC",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Verdocs/js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Verdocs/js-sdk/issues"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "docs-md": "typedoc && git add --all docs",
    "docs-html": "typedoc --plugin none --out docs-html",
    "docs": "npm run docs-md && npm run docs-html && cp -aR docs-html/* ../partner-portal/site/static/js-sdk/"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "peerDependencies": {
    "typescript": "4.0.x || 4.1.x || 4.2.x || 4.3.x || 4.4.x"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "jest": "^27.2.1",
    "jest-mock-axios": "^4.4.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.22.4",
    "typedoc-plugin-markdown": "^3.11.0",
    "typescript": "^4.3.5"
  }
}
